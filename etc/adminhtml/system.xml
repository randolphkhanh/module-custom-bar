<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="wdevs" sortOrder="300" translate="label">
			<label>Wdevs</label>
		</tab>
        <section id="custom_bar" translate="label" sortOrder="100" showInDefault="1" showInWebsite="1"
                 showInStore="1">
            <label>Custom Bar</label>
            <tab>wdevs</tab>
            <resource>Wdevs_CustomBar::config</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1"
                   showInStore="1">
                <label>General</label>
                <field id="enabled" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="1"
                       showInStore="1">
                    <label>Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
